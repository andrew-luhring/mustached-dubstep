<!DOCTYPE html>
<html ng-app="prompter">
<head>
    {{! Document Settings }}
    <base href="./">
    <meta http-equiv="Content-Type" content="text/html" charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    {{! Page Meta }}

    <meta name="HandheldFriendly" content="True" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script type="text/javascript" src="/js/lib/modernizr.js"></script>
    {{! Styles'n'Scripts }}
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
	<script type="text/javascript" src="/js/lib/angular.js"></script>
	<script type="text/javascript">
        var prompter= angular.module('prompter', []).controller('prompterController', function($scope) {});
        prompter.config(function($interpolateProvider) {
            $interpolateProvider.startSymbol('[[');
            $interpolateProvider.endSymbol(']]');
        });
	</script>

</head>
<body >
    {{! Everything else gets inserted here }}
    {{> site-head}}
    {{> index}}

<footer id="footer" class="cf site-footer">
        <div>
            <p class="button">
                <a href="http://www.androo.pro">&#35;swag</a>
            </p>
        </div>
    </footer>
    <script type="text/javascript" src="/js/lib/jquery.js"></script>
    <script type="text/javascript" src="/js/lib/jqueryui.js"></script>
    <script type="text/javascript" src="/js/lib/jqTransit.js"></script>
    <script type="text/javascript" src="/js/lib/mousewheel.js"></script>
    <script type="text/javascript" src="/js/prompter.js"></script>
</body>
</html>
